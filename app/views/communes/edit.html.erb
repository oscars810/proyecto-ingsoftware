<h1>Editar Comuna</h1>

<%= form_with(model: @comuna, url: comuna_path(@comuna.id), method: :patch) do |form| %>
  <div class="field">
    <%= form.label :nombre %>
    <%= form.text_field :nombre %>
  </div>

  <div class="actions">
    <%= form.submit 'Guardar'%>
  </div>
<% end %>

<%= link_to 'Volver', comuna_path(@comuna.id) %>